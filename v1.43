function buildJourneyMenu() {
    const rawLinks = document.querySelectorAll('.raw-link');
    const menuContainer = document.getElementById('dynamic-journey-menu');
    
    if (!rawLinks.length || !menuContainer) return;
    
    menuContainer.innerHTML = ''; 
    let currentSubArea = null;

    rawLinks.forEach(link => {
        let name = link.innerHTML.trim();
        const href = link.getAttribute('data-href');

        const isSub = /(&#\d+;|â”•|â””|â”—)/.test(name);
        const cleanName = name.replace(/(&#\d+;|â”•|â””|â”—)/g, '').trim();

        if (isSub) {
            if (currentSubArea) {
                const subA = document.createElement('a');
                subA.className = 'sub-link-item';
                subA.href = href;
                subA.innerHTML = cleanName;
                currentSubArea.appendChild(subA);
            }
        } else {
            const group = document.createElement('div');
            group.className = 'journey-group';
            
            const btn = document.createElement('div');
            btn.className = 'main-cat';
            
            // FIX: Wraps the name in an <a> tag so it is clickable
            btn.innerHTML = `
                <a href="${href}" style="text-decoration:none; color:inherit; flex-grow:1;">${cleanName}</a>
                <span class="toggle-sym" style="padding-left:10px;">+</span>`;
            
            const wrapper = document.createElement('div');
            wrapper.className = 'sub-menu-container';
            
            // FIX: Only the + symbol toggles the menu, the text opens the link
            btn.querySelector('.toggle-sym').onclick = function(e) {
                e.preventDefault();
                e.stopPropagation();
                const isOpen = wrapper.classList.toggle('open');
                this.innerText = isOpen ? 'âˆ’' : '+';
            };
            
            group.appendChild(btn);
            group.appendChild(wrapper);
            menuContainer.appendChild(group);
            currentSubArea = wrapper;
        }
    });
}

buildJourneyMenu();
window.addEventListener('load', buildJourneyMenu);
//]]>
</script>
<script type='text/javascript'>
//<![CDATA[
(function() {
    var target = document.getElementById("dynamic-topics");
    if (!target) return;

    // This "Watcher" looks at your sidebar every 500ms
    var autoSync = setInterval(function() {
        // It looks for the actual labels you have in your sidebar
        var sidebarLabels = document.querySelectorAll(".label-name, .Label a");
        
        if (sidebarLabels.length > 0) {
            target.innerHTML = ""; // Clear the area
            
            // It takes the first 2 topics found in your sidebar
            for (var i = 0; i < 2 && i < sidebarLabels.length; i++) {
                var originalText = sidebarLabels[i].innerText || sidebarLabels[i].textContent;
                // Remove numbers like (1) and take only the first word
                var cleanWord = originalText.replace(/\(\d+\)/g, "").trim().split(" ")[0].toUpperCase();
                
                var a = document.createElement("a");
                // LINK STATUS: This targets the Blogger main content area
                a.href = "#main"; 
                a.className = "dynamic-link";
                a.style.cssText = "color:inherit; text-decoration:none; font-weight:800; font-size:12px; letter-spacing:1px; cursor:pointer;";
                a.innerText = cleanWord;
                
                target.appendChild(a);
            }
            clearInterval(autoSync); // Stop searching once it finds your topics
            console.log("Automation Active: Header synced with sidebar.");
        }
    }, 500);
})();
//]]>
</script>
    <script type='text/javascript'>
//<![CDATA[
document.addEventListener("DOMContentLoaded", function () {

  var sidebarLinks = document.querySelectorAll(
    '.widget-content a[href^="#"], .LinkList a[href^="#"], aside a[href^="#"]'
  );

  if (!sidebarLinks || sidebarLinks.length < 2) return;

  var ids = [];
  for (var i = 0; i < sidebarLinks.length; i++) {
    var href = sidebarLinks[i].getAttribute("href");
    if (href && href.indexOf("#") === 0) {
      ids.push(href.replace("#", ""));
    }
  }

  var article = document.querySelector("article, main");
  if (!article) return;

  for (var j = 0; j < ids.length; j++) {
    var section = article.querySelector("#" + ids[j]);
    if (!section) continue;

    if (
      section.nextElementSibling &&
      section.nextElementSibling.classList.contains("section-nav")
    ) continue;

    var nav = document.createElement("div");
    nav.className = "section-nav";

    if (j > 0) {
      var prev = document.createElement("a");
      prev.href = "#" + ids[j - 1];
      prev.innerHTML = "&#8592; Previous";
      nav.appendChild(prev);
    } else {
      nav.appendChild(document.createElement("span"));
    }

    if (j < ids.length - 1) {
      var next = document.createElement("a");
      next.href = "#" + ids[j + 1];
      next.innerHTML = "Next &#8594;";
      nav.appendChild(next);
    }

    var cursor = section;
    while (
      cursor.nextElementSibling &&
      ids.indexOf(cursor.nextElementSibling.id) === -1
    ) {
      cursor = cursor.nextElementSibling;
    }

    cursor.parentNode.insertBefore(nav, cursor.nextSibling);
  }
});
//]]>
</script>

