  position: relative;
  z-index: 1;
}

/* 4. Optional: soften scrollbar (nice UX) */
aside::-webkit-scrollbar {
  width: 6px;
}
aside::-webkit-scrollbar-thumb {
  background: #cbd5e1;
  border-radius: 4px;
}
/* ===============================
   FINAL MOBILE FIX (IMPORTANT)
   =============================== */

@media (max-width: 900px) {

  
  /* Stack all columns */
  .main-grid > aside,
  .main-grid > main {
    width: 100% !important;
    height: auto !important;
    position: static !important;
    overflow: visible !important;
  }

  /* Disable sticky logic on mobile */
  aside {
    position: static !important;
    top: auto !important;
    height: auto !important;
  }

  /* Order: MAIN â†’ LEFT â†’ RIGHT */
  .main-grid {
    display: flex !important;
    flex-direction: column !important;
  }

  .main-grid > main {
    order: 1;
  }

  .main-grid > aside:first-of-type {
    order: 2; /* left sidebar */
  }

  .main-grid > aside:last-of-type {
    order: 3; /* right sidebar */
  }

  /* Header spacing safety */
  body {
    padding-top: 70px !important;
  }
}

/* =========================================
   AUTO NEXT / PREVIOUS â€“ SECTION NAV
   (GeeksforGeeks style)
   ========================================= */

.section-nav {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 32px 0;
  padding: 14px 0;
  border-top: 1px solid #e5e7eb;
  border-bottom: 1px solid #e5e7eb;
}

.section-nav a {
  font-size: 14px;
  font-weight: 600;
  color: var(--brand-blue, #2563eb);
  text-decoration: none;
}

.section-nav a:hover {
  text-decoration: underline;
}

.section-nav span {
  visibility: hidden; /* keeps spacing when no Previous */
}

/* ---------- Mobile friendly ---------- */
@media (max-width: 768px) {
  .section-nav {
    flex-direction: column;
    gap: 10px;
    text-align: center;
  }
}

/* [PATCH 2.2 â€“ LOGO RENDER PRIORITY FIX | v1.41] */
#header-container img {
  height:110px !important;
  width:auto !important;
  max-width:none !important;
  opacity:1 !important;
  visibility:visible !important;
}


/* ===== FINAL PAGE STATE CONTROL ===== */

/* HOME PAGE */
body.index {
  background:
    linear-gradient(rgba(248,250,252,0.9), rgba(248,250,252,0.9)),
    url("https://images.unsplash.com/photo-1518770660439-4636190af475")
    center / cover no-repeat fixed !important;
}

body.index #sidebar-left { display: none !important; }
body.index #sidebar-right { display: block !important; }

/* POST / STUDY PAGE */
body.item-view {
  background: #ffffff !important;
}

body.item-view #sidebar-left { display: block !important; }
body.item-view #sidebar-right { display: none !important; }

body.item-view .main-grid {
  grid-template-columns: 260px 1fr !important;
}



